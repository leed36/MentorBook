    <TextView
        android:id="@+id/test8"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/test7"
        android:text="test"
        />

    <Button
        android:id="@+id/test"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/test8"
        android:text="Test"/>




</RelativeLayout>
